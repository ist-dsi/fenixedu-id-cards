{
  "name": "fenixedu-id-cards-frontend",
  "version": "1.0.0",
  "description": "The FenixEdu Id Cards frontend application",
  "author": "DSI <dsi@tecnico.ulisboa.pt>",
  "private": true,
  "scripts": {
    "mock": "cross-env MOCK=true webpack-dev-server --hot --inline --config build/webpack.conf.dev.js",
    "dev": "webpack-dev-server --hot --inline --config build/webpack.conf.dev.js",
    "start": "npm run dev",
    "dist": "mkdir -p ../webapp/tecnico-card; rm -rf ../webapp/tecnico-card/*; rm -rf dist; webpack --config build/webpack.conf.prod.js; cp dist/* ../webapp/tecnico-card/",
    "lint": "eslint --ext .js,.vue src --fix",
    "analysis": "webpack --mode=production --display-reasons --json --config build/webpack.conf.prod.js > build/analysis.json",
    "prodServer": "http-server dist/",
    "clean": "rm -rf node_modules package-lock.json"
  },
  "dependencies": {
    "axios": "0.17.0",
    "v-scroll-lock": "^1.0.1",
    "vue": "2.5.17",
    "vue-click-outside": "^1.0.7",
    "vue-router": "3.0.1",
    "vue2-touch-events": "^2.0.0",
    "vuex": "3.0.1",
    "vuex-i18n": "1.10.5"
  },
  "devDependencies": {
    "autoprefixer": "7.1.2",
    "babel-core": "6.22.1",
    "babel-eslint": "9.0.0",
    "babel-helper-vue-jsx-merge-props": "2.0.3",
    "babel-loader": "7.1.1",
    "babel-plugin-syntax-jsx": "6.18.0",
    "babel-plugin-transform-runtime": "6.22.0",
    "babel-plugin-transform-vue-jsx": "3.5.0",
    "babel-preset-env": "1.3.2",
    "babel-preset-stage-2": "6.22.0",
    "chalk": "2.0.1",
    "copy-webpack-plugin": "4.5.1",
    "cross-env": "^5.2.0",
    "css-loader": "0.28.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-vue": "^4.0.0",
    "file-loader": "1.1.11",
    "friendly-errors-webpack-plugin": "1.7.0",
